{
	"version": 2,
	"build": [
		{
			"src": "./index.js",
			"use":"@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/",
			"dest": "/public/index.html"
		},
		{
			"src": "/index2",
			"dest": "/public/index2.html"
		},
		{
			"src": "/(.*)",
			"status": 404,
			"dest": "/public/404.html"
		}
	]
}


